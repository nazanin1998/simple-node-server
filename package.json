    "cookie-parser": "~1.4.4",
    "create-react-class": "^15.7.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "express-react-views": "^0.11.0",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "js": "^0.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.4",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15",
    "pbkdf2-password": "^1.2.1",
    "pug": "2.0.0-beta11",
    "react-router-dom": "^6.2.2"
  },

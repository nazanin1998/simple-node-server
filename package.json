{
  "name": "simple-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./build/server.js",
    "start:dev": "nodemon --exec babel-node ./server.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server.js -s",
    "build": "npm run clean && npm run build-babel"
  },
